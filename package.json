{
	"name": "rheinklang-kollektiv",
	"version": "2.1.37",
	"scripts": {
		"postinstall": "npx npm-run-all postinstall:*",
		"postinstall:ngcc": "ngcc",
		"postinstall:clean-locales": "node bin/clean-locales.js",
		"ng": "ng",
		"prestart": "run-s create:*",
		"start": "ng serve --host 127.0.0.1 --disable-host-check",
		"create:env": "npx npm-run-all create:env:*",
		"create:env:dev": "node bin/create-env",
		"create:env:prod": "node bin/create-env --environment=production",
		"create:version": "node bin/create-version",
		"create:license": "npx license-checker --out ./src/server/3rdpartylicenses.txt --csv --relativeLicensePath --production",
		"prebuild": "run-s create:*",
		"build": "ng build",
		"bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer ./dist/stats-es2015.json",
		"test": "echo 'No tests defined'",
		"lint": "ng lint",
		"commit": "git-cz",
		"preproduction": "run-s create:version",
		"production": "ng build --configuration=production --aot=true --buildOptimizer=true --optimization=true",
		"postproduction": "npx npm-run-all bin:generate-sitemap bin:copy-files",
		"pipeline": "npx npm-run-all pipeline:*",
		"pipeline:version": "npm run create:version",
		"pipeline:env": "npm run create:env",
		"pipeline:build": "npm run production",
		"prettier": "prettier --write \"**/*.*(js|jsx|ts|tsx|json|md)\"",
		"deploy:development": "node deploy.js --tag=development",
		"deploy:production": "node deploy.js --tag=production",
		"deploy:local:development": "node -r dotenv/config deploy.js --tag=development",
		"deploy:local:production": "node -r dotenv/config deploy.js --tag=production",
		"bin:copy-files": "node bin/copy-files",
		"bin:generate-sitemap": "node bin/sitemap-generator",
		"pwa-assets": "node bin/pwa-asset-generator.js",
		"changelog": "run-s changelog:*",
		"changelog:clean": "rimraf CHANGELOG.md",
		"changelog:generate": "npx generate-changelog"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~10.1.5",
		"@angular/cdk": "~8.2.0",
		"@angular/common": "~10.1.5",
		"@angular/compiler": "~10.1.5",
		"@angular/core": "~10.1.5",
		"@angular/forms": "~10.1.5",
		"@angular/material": "~8.2.0",
		"@angular/material-moment-adapter": "~8.2.0",
		"@angular/platform-browser": "~10.1.5",
		"@angular/platform-browser-dynamic": "~10.1.5",
		"@angular/router": "~10.1.5",
		"@angular/service-worker": "~10.1.5",
		"@apollo/client": "3.2.2",
		"@ionic/storage": "2.3.1",
		"@ngneat/content-loader": "5.0.0",
		"apollo-angular": "2.0.4",
		"apollo-angular-link-http": "1.11.0",
		"apollo-cache-inmemory": "1.6.6",
		"apollo-cache-persist": "0.1.1",
		"apollo-client": "2.6.10",
		"apollo-link": "1.2.14",
		"apollo-link-error": "1.1.13",
		"graphql": "15.3.0",
		"graphql-tag": "2.11.0",
		"lazysizes": "5.2.2",
		"masonry-layout": "4.2.2",
		"microtip": "0.2.2",
		"moment": "2.29.1",
		"normalize.css": "8.0.1",
		"rxjs": "~6.5.0",
		"svgxuse": "1.2.6",
		"swiper": "5.3.6",
		"tslib": "^2.0.0",
		"ua-parser-js": "0.7.21",
		"zone.js": "~0.10.2"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.1001.4",
		"@angular/cli": "~10.1.4",
		"@angular/compiler-cli": "~10.1.5",
		"@angular/language-service": "~10.1.5",
		"@types/fs-extra": "8.1.0",
		"@types/googlemaps": "3.39.2",
		"@types/jasmine": "~3.3.8",
		"@types/jasminewd2": "~2.0.3",
		"@types/masonry-layout": "4.2.1",
		"@types/node": "^12.11.1",
		"@types/ol": "6.1.0",
		"@types/swiper": "5.2.1",
		"@types/ua-parser-js": "0.7.33",
		"axios": "0.19.2",
		"chalk": "2.4.2",
		"codelyzer": "^5.1.2",
		"dotenv": "8.1.0",
		"ftp-deploy": "2.3.3",
		"git-cz": "3.2.1",
		"git-describe": "4.0.4",
		"globby": "11.0.0",
		"husky": "4.2.3",
		"jasmine-core": "~3.5.0",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "~5.0.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~3.0.2",
		"karma-jasmine": "~4.0.0",
		"karma-jasmine-html-reporter": "^1.5.0",
		"lint-staged": "10.0.7",
		"npm-run-all": "4.1.5",
		"ora": "3.4.0",
		"prettier": "1.18.2",
		"protractor": "~7.0.0",
		"pwa-asset-generator": "2.3.0",
		"rimraf": "3.0.2",
		"ts-node": "~7.0.0",
		"tslint": "~6.1.0",
		"typescript": "~4.0.3",
		"webpack-bundle-analyzer": "3.7.0"
	},
	"config": {
		"commitizen": {
			"path": "git-cz"
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "npm run lint"
		}
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,md}": ["prettier --write", "git add"]
	}
}
